@model Part52Mvc_.Models.Employee

<table style="border:1px solid black">
    <tr>
        <td>
            <img src="@Url.Content(Model.Photo)" alt="@Model.AlternativeText" style="width:100px; height:100px;" />
        </td>
        <td>
            <table>
                <tr><td><b>AGE:</b></td><td>@Model.Age</td></tr>
                <tr><td><b>GENDER:</b></td><td>@Model.Gender</td></tr>
                <tr><td><b>SALARY:</b></td><td>@Model.Salary</td></tr>
            </table>
        </td>
    </tr>
</table>

